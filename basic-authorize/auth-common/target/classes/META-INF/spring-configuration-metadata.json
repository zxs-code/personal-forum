      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig"
    },
    {
      "name": "global.config.auth.token.friendly-expire",
      "type": "java.lang.Boolean",
      "description": "是否开启友好过期，将refreshToken过期设置在当天凌晨3点",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": false
    },
    {
      "name": "global.config.auth.token.jwt.pri-key-path",
      "type": "java.lang.String",
      "description": "私钥文件路径",
      "sourceType": "com.github.code.zxs.auth.config.JwtConfig",
      "defaultValue": "priKey"
    },
    {
      "name": "global.config.auth.token.jwt.private-key",
      "type": "java.security.PrivateKey",
      "description": "私钥",
      "sourceType": "com.github.code.zxs.auth.config.JwtConfig"
    },
    {
      "name": "global.config.auth.token.jwt.pub-key-path",
      "type": "java.lang.String",
      "description": "公钥文件路径",
      "sourceType": "com.github.code.zxs.auth.config.JwtConfig",
      "defaultValue": "pubKey"
    },
    {
      "name": "global.config.auth.token.jwt.public-key",
      "type": "java.security.PublicKey",
      "description": "公钥",
      "sourceType": "com.github.code.zxs.auth.config.JwtConfig"
    },
    {
      "name": "global.config.auth.token.jwt.secret",
      "type": "java.lang.String",
      "description": "secret 作为 随机数的种子，用来创建公钥私钥文件（文件存在时，不会再创建）",
      "sourceType": "com.github.code.zxs.auth.config.JwtConfig",
      "defaultValue": "12345"
    },
    {
      "name": "global.config.auth.token.refresh-cookie-domain",
      "type": "java.lang.String",
      "description": "refresh_token在cookie中的名字",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "localhost"
    },
    {
      "name": "global.config.auth.token.refresh-cookie-max-age",
      "type": "java.lang.Long",
      "description": "refresh_token cookie有效时长,单位：秒 与token默认过期时间匹配，不随refreshTokenExpire改变而改变(配置文件属性注入在初始化之后)。",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig"
    },
    {
      "name": "global.config.auth.token.refresh-cookie-name",
      "type": "java.lang.String",
      "description": "refresh_token在cookie中的名字",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "refresh_token"
    },
    {
      "name": "global.config.auth.token.refresh-cookie-path",
      "type": "java.lang.String",
      "description": "refresh_token cookie Path,Path及子路径会携带该cookie",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "\/basic-authorize\/auth\/refresh"
    },
    {
      "name": "global.config.auth.token.refresh-token-expire",
      "type": "java.lang.Integer",
      "description": "refreshToken有效时长",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": 30
    },
    {
      "name": "global.config.auth.token.refresh-unit",
      "type": "java.util.concurrent.TimeUnit",
      "description": "refresh_token有效时长单位",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig"
    },
    {
      "name": "global.config.auth.token.token-model",
      "type": "com.github.code.zxs.auth.enums.TokenModel",
      "description": "使用的令牌模型，默认为 jwt",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig"
    },
    {
      "name": "global.config.core.task.pool.core-pool-size",
      "type": "java.lang.Integer",
      "description": "核心线程数",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "defaultValue": 0
    },
    {
      "name": "global.config.core.task.pool.enable",
      "type": "java.lang.Boolean",
      "description": "是否覆盖默认异步线程池",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "defaultValue": false
    },
    {
      "name": "global.config.core.task.pool.keep-alive-seconds",
      "type": "java.lang.Integer",
      "description": "线程存活时间",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "defaultValue": 0
    },
    {
      "name": "global.config.core.task.pool.max-pool-size",
      "type": "java.lang.Integer",
      "description": "最大线程数",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "defaultValue": 0
    },
    {
      "name": "global.config.core.task.pool.queue-capacity",
      "type": "java.lang.Integer",
      "description": "等待队列最大任务数",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "defaultValue": 0
    }
