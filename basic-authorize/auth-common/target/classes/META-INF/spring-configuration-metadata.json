    },
    {
      "name": "global.config.auth.token.uuid",
      "type": "com.github.code.zxs.auth.config.UUIDTokenConfig",
      "sourceType": "com.github.code.zxs.auth.config.UUIDTokenConfig"
    },
    {
      "name": "global.config.core.task.pool",
      "type": "com.github.code.zxs.auth.config.TaskThreadPoolConfig",
      "sourceType": "com.github.code.zxs.auth.config.TaskThreadPoolConfig"
    }
