  "properties": [
    {
      "name": "global.config.auth.mobile-max-device",
      "type": "java.lang.Integer",
      "description": "移动端最大登录设备数，默认为 1",
      "sourceType": "com.github.code.zxs.auth.config.AuthConfig",
      "defaultValue": 1
    },
    {
      "name": "global.config.auth.pc-max-device",
      "type": "java.lang.Integer",
      "description": "PC端最大登录设备数，默认为 5",
      "sourceType": "com.github.code.zxs.auth.config.AuthConfig",
      "defaultValue": 5
    },
    {
      "name": "global.config.auth.token.access-cookie-domain",
      "type": "java.lang.String",
      "description": "access_token在cookie中的名字",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "localhost"
    },
    {
      "name": "global.config.auth.token.access-cookie-max-age",
      "type": "java.lang.Long",
      "description": "access_token cookie有效时长,单位：秒。 与token默认过期时间匹配，不随accessTokenExpire改变而改变(配置文件属性注入在初始化之后)。",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig"
    },
    {
      "name": "global.config.auth.token.access-cookie-name",
      "type": "java.lang.String",
      "description": "access_token在cookie中的名字",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "access_token"
    },
    {
      "name": "global.config.auth.token.access-cookie-path",
      "type": "java.lang.String",
      "description": "access_token cookie Path,Path及子路径会携带该cookie",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": "\/"
    },
    {
      "name": "global.config.auth.token.access-token-expire",
      "type": "java.lang.Integer",
      "description": "accessToken有效时长",
      "sourceType": "com.github.code.zxs.auth.config.TokenGlobalConfig",
      "defaultValue": 120
    },
    {
      "name": "global.config.auth.token.access-unit",
      "type": "java.util.concurrent.TimeUnit",
